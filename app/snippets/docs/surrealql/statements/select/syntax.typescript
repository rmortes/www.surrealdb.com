SELECT [ VALUE ] @fields [ AS @alias ]
	FROM @targets
	[ WHERE @conditions ]
	[ SPLIT [ AT ] @field ... ]
	[ GROUP [ BY ] @fields ... ]
	[ ORDER [ BY ]
		@fields [
			RAND()
			| COLLATE
			| NUMERIC
		] [ ASC | DESC ] ...
	] ]
	[ LIMIT [ BY ] @limit ]
	[ START [ AT ] @start ]
	[ FETCH @fields ... ]
	[ TIMEOUT @duration ]
	[ PARALLEL ]
	[ EXPLAIN [ FULL ]]
;
