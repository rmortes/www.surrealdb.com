REMOVE [
	NAMESPACE @name
	| DATABASE @name
	| USER @name ON [ ROOT | NAMESPACE | DATABASE ]
	| LOGIN @name ON [ NAMESPACE | DATABASE ]
	| TOKEN @name ON [ NAMESPACE | DATABASE ]
	| SCOPE @name
	| TABLE @name
	| EVENT @name ON [ TABLE ] @table
	| FUNCTION fn::@name
	| FIELD @name ON [ TABLE ] @table
	| INDEX @name ON [ TABLE ] @table
	| PARAM $@name
]
